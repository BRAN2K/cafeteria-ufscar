{
  "output": {
    "filePath": "repomix-output.md",
    "style": "markdown",
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false
  },
  "include": [
    "src/**/*.ts",
    "knexfile.ts",
    "docker-compose.db.yml",
    "Dockerfile",
    "package.json",
    "docker-compose.yaml",
    "tsconfig.json"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": []
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
